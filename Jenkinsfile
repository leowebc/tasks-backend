pipeline {
    agent any
    stages {
        stage ('Início'){
            steps {
                bat ' echo início '
            }
        }
        stage ('Meio'){
            steps {
                bat ' echo maio '
                bat ' echo maio '
            }
        }
        stage ('Fim'){
            steps {
                sleep(5)
                bat ' echo fim! '
            }
        }
        stage ('Fim 2 '){
            steps {               
                bat ' echo fim! fIM MMMMMMM '
            }
        }
        stage (' Teste'){
            steps {               
                bat ' erroooooooo '
            }
        }
    }
}